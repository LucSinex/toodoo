<%= form_for @user_session do |f| %>
  <div class="form_field">
  	<%= f.label :login %>
  	<%= f.text_field :login %>
  </div>
  <div class="form_field">
  	<%= f.label :password %>
  	<%= f.password_field :password %>
  </div>  
  <div class="form_field">
  	<%= f.submit "Submit" %>
  </div>
<% end %>